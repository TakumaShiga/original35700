<div class="contents row">
  <div class="container">
   <% @questions.each do |question| %>
    <p>
      <%= link_to question.title, question_path(question) %>
      <%= link_to question.user.nickname, user_path(question.user.id) %>
      <%= question.created_at %>
    </p>
   <% end %>
  </div>
</div>